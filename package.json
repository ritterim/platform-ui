{
  "name": "@ritterim/platform-ui",
  "version": "0.9.33",
  "description": "A CSS framework used by Ritter Insurance Marketing (RIM) for our platform of agent tools.",
  "homepage": "https://style.rimdev.io",
  "main": "gulpfile.js",
  "files": [
    "/dist"
  ],
  "modules": {
    "name": "platform icons",
    "version": "0.4.0"
  },
  "scripts": {
    "start": "npm install && gulp",
    "build": "gulp build",
    "generate-assets": "icon-font-generator src/assets/icons/svg/**/*.svg -o dist -n platform-icons -j -p pi --center --htmltp templates/html.hbs --csstp templates/css.hbs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver": "echo \"$npm_package_version\"",
    "nightwatch": "percy exec -- nightwatch",
    "browser-visual-tests": "npm run build && npm run nightwatch",
    "nightwatch:debug": "node --inspect-brk ./node_modules/nightwatch/bin/runner.js -t ./browser_automated_tests/docs-pages.js"
  },
  "browserslist": [
    "last 2 version",
    "> 2%",
    "maintained node versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ritterim/platform-ui.git"
  },
  "keywords": [
    "CSS",
    "SASS",
    "UI"
  ],
  "author": "Ritter Insurance Marketing (https://rimdev.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritterim/platform-ui/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@percy/nightwatch": "^1.0.5",
    "autoprefixer": "^9.6.0",
    "chromedriver": "^78.0.1",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-header": "^2.0.9",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "http-server": "^0.12.0",
    "icon-font-generator": "^2.1.10",
    "kss": "^3.0.0-beta.25",
    "merge-stream": "^2.0.0",
    "nightwatch": "^1.3.1",
    "sass": "^1.23.1",
    "selenium-server": "^3.141.59"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 2,
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "semver": "^6.3.0"
  }
}
