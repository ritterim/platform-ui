{
  "name": "@ritterim/platform-ui",
  "version": "1.5.6",
  "description": "A CSS framework used by Ritter Insurance Marketing (RIM) for our platform of agent tools.",
  "homepage": "https://style.rimdev.io",
  "main": "gulpfile.js",
  "files": [
    "/dist",
    "/src"
  ],
  "scripts": {
    "start": "npm install && gulp",
    "build": "gulp build",
    "generate-assets": "icon-font-generator src/assets/icons/svg/**/*.svg -o dist -n platform-icons -j -p pi --center --htmltp templates/html.hbs --csstp templates/css.hbs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver": "echo \"$npm_package_version\"",
    "deploy": "./deploy-to-npm.sh"
  },
  "browserslist": [
    "last 2 version",
    "> 2%",
    "maintained node versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ritterim/platform-ui.git"
  },
  "keywords": [
    "CSS",
    "SASS",
    "UI"
  ],
  "author": "Ritter Insurance Marketing (https://rimdev.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritterim/platform-ui/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "cssnano": "^5.0.10",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-header": "^2.0.9",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "icon-font-generator": "^2.1.10",
    "kss": "3.0.0-beta.25",
    "merge-stream": "^2.0.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.3.11",
    "sass": "^1.25.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 2,
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "semver": "^6.3.0"
  }
}
