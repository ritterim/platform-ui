{
  "name": "@ritterim/platform-ui",
  "version": "0.8.9",
  "description": "A CSS framework used by Ritter Insurance Marketing (RIM) for our platform of agent tools.",
  "homepage": "https://style.rimdev.io",
  "main": "gulpfile.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npm install && gulp",
    "build": "gulp build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:all": "npm install && cd custom-builder && npm install && npm run sass && cd .. && gulp",
    "build:style": "rm -rf styleguide && cd custom-builder && npm run sass && cd .. && gulp",
    "ver": "echo \"$npm_package_version\""
  },
  "browserslist": [
    "last 2 version",
    "> 2%",
    "maintained node versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ritterim/platform-ui.git"
  },
  "keywords": [
    "CSS",
    "SASS",
    "UI"
  ],
  "author": "Ritter Insurance Marketing (https://rimdev.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritterim/platform-ui/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.0",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-header": "^2.0.9",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "kss": "^3.0.0-beta.25"
  }
}
